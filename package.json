{
  "name": "vue-hello-plugin",
  "description": "A Vue.js Plugin",
  "version": "0.0.0",
  "author": {
    "name": "noistommy",
    "email": "mykim@n3n.co.kr"
  },
  "bugs": {
    "url": "https://github.com/noistommy/vue-hello-plugin/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@vue/babel-preset-app": "^3.0.1",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-istanbul": "^5.0.1",
    "babel-preset-power-assert": "^3.0.0",
    "buble": "^0.19.3",
    "conventional-changelog-cli": "^1.2.0",
    "conventional-github-releaser": "^1.1.3",
    "cross-env": "^5.1.6",
    "eslint": "^5.6.0",
    "eslint-plugin-vue-libs": "^2.0.1",
    "eslint-plugin-typescript": "^0.12.0",
    "eslint-loader": "^2.1.0",
    "mocha": "^5.2.0",
    "mocha-loader": "^2.0.0",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-coveralls": "^1.2.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-safari-launcher": "^1.0.0",
    "karma-sauce-launcher": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.5",
    "power-assert": "^1.6.0",
    "rollup": "^0.66.0",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-replace": "^2.0.0",
    "uglify-js": "^2.7.5",
    "chromedriver": "^2.27.2",
    "cross-spawn": "^6.0.0",
    "http-server": "^0.9.0",
    "nightwatch": "^0.9.12",
    "nightwatch-helpers": "^1.2.0",
    "selenium-server": "2.53.1",
    "@types/mocha": "^5.2.0",
    "@types/node": "^10.10.3",
    "@types/power-assert": "^1.5.0",
    "ts-loader": "^5.2.0",
    "fork-ts-checker-webpack-plugin": "^0.4.9",
    "typescript": "^3.0.3",
    "typescript-eslint-parser": "^18.0.0",
    "vue": "^2.5.17",
    "vue-template-compiler": "^2.5.17",
    "@vue/test-utils": "^1.0.0-beta.25",
    "vuepress": "^0.14.0",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.8"
  },
  "files": [
    "dist/vue-hello-plugin.js",
    "dist/vue-hello-plugin.min.js",
    "dist/vue-hello-plugin.common.js",
    "dist/vue-hello-plugin.esm.js",
    "types",
    "src"
  ],
  "homepage": "https://github.com/noistommy/vue-hello-plugin#readme",
  "main": "dist/vue-hello-plugin.common.js",
  "module": "dist/vue-hello-plugin.esm.js",
  "unpkg": "dist/vue-hello-plugin.js",
  "types": "types/index.d.ts",
  "keywords": [
    "plugin",
    "vue",
    "vuejs"
  ],
  "license": "MIT",
  "engines": {
    "node": ">= 8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noistommy/vue-hello-plugin.git"
  },
  "scripts": {
    "build": "npm run build:ts && node config/build.js",
    "build:ts": "tsc -p src",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p eslint",
    "release": "conventional-github-releaser -p eslint",
    "clean": "rm -rf coverage && rm -rf lib/*.js* && rm -rf types/*.d.ts* && rm -rf dist/*.js* && rm -rf ./*.log",
    "dev": "cross-env BABEL_ENV=test webpack-dev-server --inline --hot --open --content-base ./test/unit/ --config config/webpack.dev.conf.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "node config/version.js && vuepress build docs",
    "lint": "eslint --fix --ext .ts src test",
    "sauce": "npm run sauce:coolkids && npm run sauce:ie && npm run sauce:mobile",
    "sauce:coolkids": "karma start config/karma.sauce.conf.js -- 0",
    "sauce:ie": "karma start config/karma.sauce.conf.js -- 1",
    "sauce:mobile": "karma start config/karma.sauce.conf.js -- 2",
    "test": "npm run lint && npm run test:cover && npm run test:e2e",
    "test:cover": "cross-env BABEL_ENV=test karma start config/karma.cover.conf.js",
    "test:coveralls": "cross-env BABEL_ENV=test karma start config/karma.coveralls.conf.js",
    "test:e2e": "npm run build && node test/e2e/runner.js",
    "test:unit": "cross-env BABEL_ENV=test karma start config/karma.unit.conf.js"
  }
}
